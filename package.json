{
  "name": "webalizer",
  "version": "0.2.4",
  "description": "Web interface for running server commands and scripts",
  "keywords": [
    "web",
    "interface",
    "ui",
    "run",
    "scripts",
    "commands",
    "shell",
    "scripts"
  ],
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev app",
    "build": "next build app",
    "prepack": "npm run build",
    "start": "next start -p ${PORT:-3000} -H ${ADDRESS:-127.0.0.1} app",
    "jest:acceptance": "PWDEBUG=1 USERNAME=dev PASSWORD=dev PORT=3000 jest -c jest.acceptance.config.js",
    "test": "run-s test:unit test:acceptance",
    "test:unit": "jest app/**",
    "test:acceptance": "HEADLESS=true SCRIPTS_ROOT=./test/fixtures/configs BASE_URL=http://localhost:3001 USERNAME=test_user PASSWORD=test_password PORT=3001 NEXTAUTH_URL=http://localhost:3001 run-s build test:acceptance:start",
    "test:acceptance:start": "run-p -r start test:acceptance:run",
    "test:acceptance:run": "jest -c jest.acceptance.config.js test/acceptance/**",
    "test:watch": "jest --watch app/**"
  },
  "author": {
    "email": "matiassurdi@gmail.com",
    "name": "MatÃ­as Surdi",
    "url": "https://github.com/msurdi"
  },
  "homepage": "https://github.com/msurdi/webalizer",
  "bugs": {
    "email": "matiassurdi@gmail.com",
    "url": "https://github.com/msurdi/webalizer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msurdi/webalizer.git"
  },
  "license": "MIT",
  "bin": {
    "webalizer": "bin/webalizer.js"
  },
  "files": [
    "app/.next/**",
    "app/public/**",
    "bin/**",
    "next.config.js"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jest-playwright": "^0.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.4.7",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.9.1",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "execa": "^5.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
